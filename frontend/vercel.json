{
  "rewrites": [
    { 
      "source": "/((?!manifest\\.json|logo\\.png|.*\\..*).*)", 
      "destination": "/" 
    }
  ],
  "headers": [
    {
      "source": "/manifest.json",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/manifest+json"
        }
      ]
    }
  ]
}
